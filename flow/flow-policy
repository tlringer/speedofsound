# Flow policy
ACG("location") -> DISPLAY, WRITE_LOGS, DATABASE, SPEAKER

# Not implemented as ACGs yet, so we compose models
SHARED_PREFERENCES -> WRITE_LOGS, PACKAGE_INFO, SHARED_PREFERENCES, DISPLAY, SPEAKER
PACKAGE_INFO -> WRITE_LOGS, PACKAGE_INFO, SHARED_PREFERENCES
BLUETOOTH -> WRITE_LOGS, PACKAGE_INFO, SHARED_PREFERENCES, DISPLAY, SPEAKER
DATABASE -> WRITE_LOGS, DATABASE, DISPLAY
PACKAGE_INFO -> DISPLAY, SPEAKER