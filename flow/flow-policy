# Flow policy
ACG("location") -> DISPLAY
ACG("location") -> WRITE_LOGS
ACG("location") -> DATABASE

# Intents
ACG("location") -> INTENT
INTENT -> WRITE_LOGS
INTENT -> DISPLAY
INTENT -> INTENT

# Not implemented as ACGs yet, so we compose models
PACKAGE_INFO -> SHARED_PREFERENCES
SHARED_PREFERENCES -> WRITE_LOGS
PACKAGE_INFO -> WRITE_LOGS
SHARED_PREFERENCES -> PACKAGE_INFO
SHARED_PREFERENCES -> SHARED_PREFERENCES
PACKAGE_INFO -> PACKAGE_INFO
BLUETOOTH -> WRITE_LOGS
DATABASE -> WRITE_LOGS
DATABASE -> DATABASE
DATABASE -> DISPLAY
